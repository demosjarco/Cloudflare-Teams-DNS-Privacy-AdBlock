	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/demosjarco/Cloudflare-Teams-DNS-Privacy-AdBlock/issues"
	},
	"homepage": "https://cftdpa.pages.dev",
	"devDependencies": {
		"@types/semver": "^7.3.13",
		"prettier": "^2.8.7",
		"wrangler": "^2.16.0"
	},
	"dependencies": {
		"cdnjs-api": "^1.3.1",
		"semver": "^7.5.0",
		"typescript": "^5.0.4"
	}
